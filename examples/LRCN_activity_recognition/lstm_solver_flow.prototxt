net: "train_test_lstm_flow.prototxt"
test_iter: 100
test_state: { stage: 'test-on-test' }
test_interval: 100
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 1
max_iter: 70000
momentum: 0.9
weight_decay: 0.005
snapshot: 5000
snapshot_prefix: "snapshots_lstm_flow"
solver_mode: CPU
device_id: 0
random_seed: 1701
average_loss: 1000
clip_gradients: 15
